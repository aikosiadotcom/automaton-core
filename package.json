{
  "name": "@aikosia/automaton-core",
  "version": "1.2.1",
  "description": "Core Libraries Of Automaton Framework",
  "main": "./src/index.js",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/aikosiadotcom/automaton-core"
  },
  "engines": {
    "npm": ">=8.19.2",
    "node": ">=18.12.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "scripts": {
    "types": "del-cli src/**/*.ts -f && tsc",
    "docs": "cross-env NODE_ENV=production ./node_modules/.bin/jsdoc -c jsdoc.json",
    "release": "npm run types && npm run docs && git add * && git-cz && git pull origin && git push origin",
    "test": "cross-env NODE_ENV=development NODE_OPTIONS='--experimental-vm-modules' jest"
  },
  "keywords": [
    "aikosia",
    "automaton",
    "automata",
    "automation",
    "bot",
    "puppeteer",
    "playwright",
    "chromium",
    "framework",
    "deployment",
    "infrastructure",
    "shareable",
    "core",
    "library",
    "runtime"
  ],
  "files": [
    "src/**/*.js",
    "src/**/*.d.ts",
    "!src/**/*.test.js",
    "!src/mocks/**/*"
  ],
  "author": "firman",
  "license": "MIT",
  "imports": {
    "#src/*": "./src/*.js",
    "#feature/*": "./src/features/*.js",
    "#logger/*": "./src/features/logger/*.js",
    "#runtime/*": "./src/runtimes/*.js",
    "#bot_loader/*": "./src/runtimes/bot_loader/*.js",
    "#compiler/*": "./src/runtimes/compiler/*.js",
    "#error/*": "./src/errors/*.js",
    "#mock": "./src/mocks",
    "#mock/*": "./src/mocks/*.js"
  },
  "exports": "./src/index.js",
  "dependencies": {
    "@babel/core": "^7.21.4",
    "@babel/plugin-proposal-decorators": "^7.21.0",
    "@babel/preset-env": "^7.21.4",
    "@igor.dvlpr/valid-path": "^2.2.1",
    "@supabase/supabase-js": "^2.21.0",
    "@trenskow/parallel": "^0.1.3",
    "axios": "^1.3.6",
    "deep-freeze-es6": "^2.0.0",
    "delay": "^5.0.0",
    "dotenv": "^16.0.3",
    "env-paths": "^3.0.0",
    "extend": "^3.0.2",
    "fs-extra": "^11.1.1",
    "import-meta-resolve": "^3.0.0",
    "json-validator": "^0.0.34",
    "node-cron": "^3.0.2",
    "node-envchecker": "^1.0.8",
    "p-retry": "^5.1.2",
    "playwright-core": "^1.32.3",
    "pretty-hrtime": "^1.0.3",
    "random": "^4.1.0",
    "read-pkg": "^8.0.0",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^8.0.7",
    "@semantic-release/npm": "^10.0.3",
    "@semantic-release/release-notes-generator": "^11.0.1",
    "better-docs": "github:aikosiadotcom/better-docs#dev",
    "commitizen": "^4.3.0",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "del-cli": "^5.0.0",
    "jest": "^29.5.0",
    "jest-html-reporters": "^3.1.4",
    "jsdoc": "^4.0.2",
    "jsdoc-plugin-intersection": "^1.0.4",
    "node-jest-badges": "^1.3.12",
    "semantic-release": "^21.0.2",
    "taffydb": "^2.7.3",
    "typescript": "^5.0.4"
  }
}
